msgid "magit-git-command"
msgstr ""

#. type: table
#: orig/magit.texi:2465 orig/magit.texi:2481
msgid ""
"This command reads a command from the user and executes it in "
"@code{default-directory}.  With a prefix argument the command is executed in "
"the top-level directory of the current working tree instead."
msgstr ""

#. type: item
#: orig/magit.texi:2469
#, no-wrap
msgid "@kbd{! s} (@code{magit-shell-command-topdir})"
msgstr ""

#. type: kindex
#: orig/magit.texi:2470
#, no-wrap
msgid "! s"
msgstr ""

