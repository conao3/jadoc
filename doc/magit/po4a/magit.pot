#. type: findex
#: orig/magit.texi:1278
#, no-wrap
msgid "magit-section-backward"
msgstr ""

#. type: table
#: orig/magit.texi:1282
msgid ""
"When not at the beginning of a section, then move to the beginning of the "
"current section.  At the beginning of a section, instead move to the "
"beginning of the previous visible section."
msgstr ""

#. type: item
#: orig/magit.texi:1283
#, no-wrap
msgid "@kbd{n} (@code{magit-section-forward})"
msgstr ""

#. type: kindex
#: orig/magit.texi:1284 orig/magit.texi:5222 orig/magit.texi:7240
#: orig/magit.texi:9784
#, no-wrap
msgid "n"
msgstr ""

#. type: findex
#: orig/magit.texi:1285
#, no-wrap
msgid "magit-section-forward"
msgstr ""

#. type: table
#: orig/magit.texi:1287
msgid "Move to the beginning of the next visible section."
msgstr ""

#. type: item
#: orig/magit.texi:1288
#, no-wrap
msgid "@kbd{M-p} (@code{magit-section-backward-siblings})"
msgstr ""

#. type: kindex
#: orig/magit.texi:1289 orig/magit.texi:5917 orig/magit.texi:7245
#, no-wrap
