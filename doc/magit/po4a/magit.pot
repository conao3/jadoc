#. type: findex
#: orig/magit.texi:1290
#, no-wrap
msgid "magit-section-backward-siblings"
msgstr ""

#. type: table
#: orig/magit.texi:1294
msgid ""
"Move to the beginning of the previous sibling section.  If there is no "
"previous sibling section, then move to the parent section instead."
msgstr ""

#. type: item
#: orig/magit.texi:1295
#, no-wrap
msgid "@kbd{M-n} (@code{magit-section-forward-siblings})"
msgstr ""

#. type: kindex
#: orig/magit.texi:1296 orig/magit.texi:5924 orig/magit.texi:7250
#, no-wrap
msgid "M-n"
msgstr ""

#. type: findex
#: orig/magit.texi:1297
#, no-wrap
msgid "magit-section-forward-siblings"
msgstr ""

#. type: table
#: orig/magit.texi:1300
msgid ""
"Move to the beginning of the next sibling section.  If there is no next "
"sibling section, then move to the parent section instead."
msgstr ""

#. type: item
#: orig/magit.texi:1301
#, no-wrap
msgid "@kbd{^} (@code{magit-section-up})"
msgstr ""

#. type: kindex
#: orig/magit.texi:1302
#, no-wrap
