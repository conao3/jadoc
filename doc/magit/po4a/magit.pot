#: orig/magit.texi:2506
#, no-wrap
msgid "@kbd{! g} (@code{magit-run-git-gui})"
msgstr ""

#. type: kindex
#: orig/magit.texi:2507
#, no-wrap
msgid "! g"
msgstr ""

